{
  "pattern": {
    "any": {
      "value": "^\\(\\d{10}\\)\\[(?<dateTime>[:\\s\\w]{24})] (?<text>.+)$"
    },
    "zoneEnter": {
      "value": "^You have entered (?<zone>.+)\\.$"
    },
    "characterLoggedIn": {
      "value": "^(?<relation>.+): (?<character>.+) has logged in\\.?$"
    },
    "characterSkillApplied": {
      "value": "^((YOUR)|(?<character>.+)'s) (?<skill>.+) (?<action>hits|heals|absorbs|regenerates|refreshes|emboldens|reduces|increases|zaps|multi attacks) (?<rest>.+)$"
    },
    "characterSay": {
      "value": "^(?<character>.+) say(s)?( to the (?<listener>.+))?, \"(?<message>.+)\"$"
    }
  }
}